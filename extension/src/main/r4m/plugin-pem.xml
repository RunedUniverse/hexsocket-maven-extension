<project-execution-model
	xmlns="https://api.runeduniverse.net/runes4tools/r4m-pem"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://api.runeduniverse.net/runes4tools/r4m-pem 
	https://api.runeduniverse.net/runes4tools/r4m-pem-v1.xsd">

	<modelVersion>1.0.0</modelVersion>
	<executions>

		<execution source="packaging" id="default">
			<triggers>
				<default />
				<on-call />
				<packaging-procedure>hexsocket-module</packaging-procedure>
				<packaging-procedure>hexsocket-app</packaging-procedure>
			</triggers>
			<lifecycles>
				<lifecycle id="clean">
					<phases>
						<phase id="clean">
							<goals>
								<goal id="clean">
									<groupId>org.apache.maven.plugins</groupId>
									<artifactId>maven-clean-plugin</artifactId>
									<modes>
										<default />
										<dev />
									</modes>
								</goal>
							</goals>
						</phase>
					</phases>
				</lifecycle>
				<lifecycle id="default">
					<phases>
						<phase id="install">
							<goals>
								<goal id="install">
									<groupId>org.apache.maven.plugins</groupId>
									<artifactId>maven-install-plugin</artifactId>
									<modes>
										<default />
										<dev />
									</modes>
								</goal>
							</goals>
						</phase>
						<phase id="deploy">
							<goals>
								<goal id="deploy">
									<groupId>org.apache.maven.plugins</groupId>
									<artifactId>maven-deploy-plugin</artifactId>
									<modes>
										<default />
										<dev />
									</modes>
								</goal>
							</goals>
						</phase>
					</phases>
				</lifecycle>
			</lifecycles>
		</execution>

	</executions>
</project-execution-model>
